<h1>YoyoyoyoHo!</h1>

<h3>You have <%= @pictures.length %> pictures in your library.</h3>
<%= link_to "Make a New Picture", "/pictures/new" %>

<% @pictures.each do |picture| %>
	<p><strong><%= picture[:title] %></strong> by <%= picture[:artist] %></p>
	<%= link_to image_tag(picture[:url]), "/pictures/#{picture.id}" %>
<% end %>